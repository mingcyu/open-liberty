<server>

	<sslDefault sslRef="DefaultSSLSettings" />
	<ssl
		id="DefaultSSLSettings"
		keyStoreRef="myKeyStore"
		trustStoreRef="myTrustStore"
		clientAuthenticationSupported="true" />
	<keyStore
		id="myKeyStore"
		password="Liberty"
		type="PKCS12"
		location="${server.config.dir}/commonBasicKeyStore.p12" />
	<keyStore
		id="myTrustStore"
		password="Liberty"
		type="PKCS12"
		location="${server.config.dir}/commonTrustStore.p12" />

</server>        

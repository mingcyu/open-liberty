<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_0.xsd"
        version="1.0">

  <session name="RestartStartupSingletonJarBean">
    <interface class="test.ejbcontainer.restart.singleton.shared.RestartStartupSingletonRemote" binding-name="ejb/RestartStartupSingletonJarBean"/>
    <interface class="test.ejbcontainer.restart.singleton.shared.RestartStartupSingletonLocal" binding-name="ejblocal:RestartStartupSingletonJarBean"/>
  </session>

</ejb-jar-bnd>

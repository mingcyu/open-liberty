-include= ~../cnf/resources/bnd/rejar.props

bVersion=1.0

Bundle-Name: Apache Commons BeanUtils
Bundle-SymbolicName: com.ibm.ws.org.apache.commons.beanutils
Bundle-Description: Apache Commons BeanUtils; version=1.11.0

Private-Package:

# Make sure that if you use beanutils in your feature, that you also include commons collections
Import-Package: \
  org.apache.commons.collections.*, \
  *

Export-Package: \
  !org.apache.commons.collections.*, \
  org.apache.commons.beanutils.*

Include-Resource: \
    @${repo;commons-beanutils:commons-beanutils;1.11.0}!/!META-INF/MANIFEST.MF|META-INF/maven/*|META-INF/NOTICE.txt|META-INF/LICENSE.txt
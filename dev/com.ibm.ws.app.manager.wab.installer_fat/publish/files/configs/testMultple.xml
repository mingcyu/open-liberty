<server description="wab configuration server">

    <include location="../fatTestPorts.xml"/>

<!--
    Logging for issue 30315.

    See class com.ibm.ws.app.manager.wab.installer.fat.ConfigurableWABTests.java,
    test method testMultipleConfiguration.

    The test creates an unusual circumstance where the application deployment name
    is not unique.

                                                    ApplicationInfo.
                                                    getAppName()    getDeploymentName()
    [ ApplicationInfoImpl@61eb9b38[test.wab1] ]   [ test.wab1 ]   [ test.wab1 ]
    [ ApplicationInfoImpl@34c7a7c0[test.wab2] ]   [ test.wab2 ]   [ test.wab2 ]
    [ ApplicationInfoImpl@2784ac9a[test.wab2_2] ] [ test.wab2_2 ] [ test.wab2 ]
    [ ApplicationInfoImpl@c0e8062c[test.wab1_2] ] [ test.wab1_2 ] [ test.wab1 ]
  -->
<!--
    <logging
      traceSpecification="*=info=enabled:com.ibm.ws.annocache*=finer:com.ibm.ws.container.service*=finer"
      maxFileSize="20"
      maxFiles="40"
      traceFormat="BASIC"/>
 -->

    <featureManager>
        <feature>product1:product1</feature>
        <feature>product2:product2</feature>
        <feature>osgiConsole-1.0</feature>
    </featureManager>

    <product1 contextPath="/product1/wab1" contextName="WAB1" id="predefined-product1"/>
    <product1 contextPath="/product1/wab2" contextName="WAB2"/>
    <product2 contextPath="/product2/wab1" contextName="WAB1" id="predefined-product2"/>
    <product2 contextPath="/product2/wab2" contextName="WAB2"/>

</server>

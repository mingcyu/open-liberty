<server>

	<sslDefault sslRef="DefaultSSLSettings_RSA" />
	
	<ssl
		id="DefaultSSLSettings_RSA"
		keyStoreRef="rsa_key"
		trustStoreRef="rsa_trust"
		clientAuthenticationSupported="true" />

	<ssl 
		id="DefaultSSLSettings_EC" 
		keyStoreRef="ec_key" 
		trustStoreRef="ec_trust"
		clientAuthenticationSupported="true" />

	<keyStore
		id="myKeyStore"
		password="Liberty"
		type="jks"
		location="${server.config.dir}/commonBasicKeyStore.jks" />
	<keyStore
		id="myTrustStore"
		password="Liberty"
		type="jks"
		location="${server.config.dir}/commonTrustStore.jks" />

	<keyStore
		id="configServerDefault"
		password="LibertyServer"
		type="jks"
		location="${server.config.dir}/commonConfigServerDefault.jks" />

	<keyStore
		id="rsa_key"
		password="Liberty"
		type="jks"
		location="${server.config.dir}/rsa_key.jks" />

	<keyStore
		id="rsa_trust"
		password="LibertyServer"
		type="jks"
		location="${server.config.dir}/rsa_trust.jks" />

	<keyStore
		id="ec_trust"
		password="LibertyServer"
		type="PKCS12"
		location="${server.config.dir}/ec_trust.p12" />

	<keyStore
		id="ec_key"
		password="Liberty"
		type="PKCS12"
		location="${server.config.dir}/ec_key.p12" />

<include location="${shared.config.dir}/allAlgSSLSettings.xml" />

</server>        

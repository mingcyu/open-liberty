<server>

	<sslDefault sslRef="DefaultSSLSettings" />
	<ssl
		id="DefaultSSLSettings"
		keyStoreRef="myKeyStore"
		trustStoreRef="myTrustStore"
		clientAuthenticationSupported="true" />
	<keyStore
		id="myKeyStore"
		password="Liberty"
		type="PKCS12"
		location="${server.config.dir}/dsaKey.p12" />
	<keyStore
		id="myTrustStore"
		password="Liberty"
		type="PKCS12"
		location="${server.config.dir}/commonTrustStore.p12" />

	<keyStore
		id="configServerDefault"
		password="LibertyServer"
		type="PKCS12"
		location="${server.config.dir}/commonConfigServerDefault.p12" />

<include location="${shared.config.dir}/allAlgSSLSettings.xml" />

</server>        
